						regExpOne = regExpOne + "H";
					}
					if(currentTextBox == textBoxArray[1])
					{
						regExpTwo = regExpTwo + "H";
					}
					if(currentTextBox == textBoxArray[2])
					{
						regExpThree = regExpThree + "H"; 
					}
					sheepPoints--;
				}
				if(key[73]) // i
