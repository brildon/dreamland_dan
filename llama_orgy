function setPositionLvl3() {
	lvlFlag = false;
	
	distantBackground.sourceY = 1180;
	distantBackground.sourceX = 0;
	distantBackground.sourceWidth = 6336;
	distantBackground.sourceHeight = 789;
	distantBackground.width = 6336;
	distantBackground.height = 789;
	distantBackground.x = 0;
	distantBackground.y = -3;
	
	foregroundBackground.sourceY = 2854;
	foregroundBackground.sourceX = 0;
	foregroundBackground.sourceWidth = 6336;
	foregroundBackground.sourceHeight = 789;
	foregroundBackground.width = 6336;
	foregroundBackground.height = 789
	foregroundBackground.x = 0;
	foregroundBackground.y = -2;	
	
	for (var i = 0; i < breakables.length; i++){
		breakables[i].countB = 0;
		breakables[i].posX = 0;
		breakables[i].posY = 0;	
	}
	for (var i = 0; i < sheep.length; i++){
		sheep[i].posX = 0;
		sheep[i].posY = 0;	
	}
	for (var i = 0; i < sm_obj.length; i++){
		sm_obj[i].posX = 0;
		sm_obj[i].posY = 0;	
	}
	for (var i = 0; i < lg_obj.length; i++){
		lg_obj[i].posX = 0;
		lg_obj[i].posY = 0;	
	}
	for (var i = 0; i < signs.length; i++){
		signs[i].posX = 0;
		signs[i].posY = 0;	
	}
	
	miniGame.x = 0;
	miniGame.y = 0;
	
	playerObj.posX = 200;
	playerObj.posY = 600+46;
}

